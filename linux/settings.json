{
    "breadcrumbs.enabled": false,
    "debug.openDebug": "neverOpen",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "explorer.confirmDelete": false,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": false,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.allowForcePush": true,
    "git.autofetch": true,
    "git.autoStash": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "html.format.contentUnformatted": "pre,code,textarea,style",
    "html.format.endWithNewline": true,
    "html.format.maxPreserveNewLines": 0,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "php.validate.enable": false,
    "search.smartCase": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.rightClickBehavior": "copyPaste",
    "terminal.integrated.shell.linux": "/bin/bash",
    "files.associations": { "*.gyp": "jsonc" },
    "[bat]": { "files.encoding": "gb2312" },
    "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
    "[javascript]": { "editor.defaultFormatter": "HookyQR.beautify" },
    "[json]": { "editor.defaultFormatter": "HookyQR.beautify" },
    "[jsonc]": { "editor.defaultFormatter": "HookyQR.beautify" },
    "[markdown]": { "editor.fontFamily": "Sarasa Mono Slab SC Light", "editor.fontSize": 16 },
    "[vue]": { "editor.defaultFormatter": "octref.vetur" },
    "beautify.config": "~/.jsbeautifyrc",
    "beautify.language": { "js": { "type": ["javascript", "json", "jsonc"], "filename": [".jshintrc", ".jsbeautifyrc", "settings.json"] }, "css": [], "html": [] },
    "csscomb.formatOnSave": true,
    "csscomb.preset": "~/.csscomb.js",
    "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
    "eslint.alwaysShowStatus": true,
    "eslint.options": { "configFile": "/home/hs/.eslintrc.js" },
    "lebab.transforms": ["arg-spread", "arrow-return", "arrow", "class", "default-param", "destruct-param", "exponent", "for-each", "for-of", "no-strict", "obj-method"],
    "shellformat.effectLanguages": ["shellscript", "dockerfile", "dotenv", "hosts", "ignore", "gitignore", "azcli"],
    "svg.minify.sortDefsChildren": false,
    "svg.preview.mode": "svg"
}
